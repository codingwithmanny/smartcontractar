{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "with-env": "dotenv -e ../../.env --",
    "dev": "pnpm with-env next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:up": "docker compose -p turbo up -d",
    "db:down": "docker compose -p turbo down --remove-orphans -v",
    "db:migrate": "pnpm with-env tsx src/db/migrate.ts",
    "email:test": "pnpm with-env tsx src/email/send.ts"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.5.6",
    "@types/node": "20.4.5",
    "@types/pg": "^8.10.2",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "@upstash/qstash": "^0.3.6",
    "arweavekit": "^1.4.8",
    "autoprefixer": "10.4.14",
    "dotenv-cli": "^7.2.1",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.12",
    "kysely": "^0.26.1",
    "kysely-neon": "^1.3.0",
    "next": "13.4.12",
    "nodemailer": "^6.9.4",
    "pg": "^8.11.2",
    "postcss": "8.4.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.3",
    "tsx": "^3.12.7",
    "typescript": "5.1.6",
